		KNOB.BorderColor3 = Color3.fromRGB(0, 0, 0)
		KNOB.BorderSizePixel = 0
		KNOB.Position = UDim2.new(0.200000003, 0, 0.5, 0)
		KNOB.Size = UDim2.new(0.469362944, 0, 1.30041063, 0)
		KNOB.Font = Enum.Font.SourceSans
		KNOB.Text = ""
		KNOB.TextColor3 = Color3.fromRGB(0, 0, 0)
		KNOB.TextSize = 14.000

		KnobUICorner.CornerRadius = UDim.new(1, 0)
		KnobUICorner.Parent = KNOB

		KNOBUIAspectRatioConstraint.Parent = KNOB

		NewToggleBGUIAspectRatioConstraint.Parent = NewToggleBG
		NewToggleBGUIAspectRatioConstraint.AspectRatio = 1.918

		NewToggleUIAspectRatioConstraint.Parent = NewToggle
		NewToggleUIAspectRatioConstraint.AspectRatio = 8.086

		KNOB.MouseButton1Click:Connect(function()
			KNOB:SetAttribute("Toggle", not KNOB:GetAttribute("Toggle"))

			if KNOB:GetAttribute("Toggle") == true then
				tweenService:Create(KNOB, tweenInfo, {Position = UDim2.new(0.8, 0, 0.5, 0)}):Play()
				tweenService:Create(KNOB.Parent, tweenInfo, {BackgroundColor3 = Color3.fromRGB(0, 22, 42)}):Play()
			else
				tweenService:Create(KNOB, tweenInfo, {Position = UDim2.new(0.2, 0, 0.5, 0)}):Play()
				tweenService:Create(KNOB.Parent, tweenInfo, {BackgroundColor3 = Color3.fromRGB(47, 47, 47)}):Play()
			end

			if callback then
				callback(KNOB:GetAttribute("Toggle"))
			end
		end)
	end
end

return shitware
