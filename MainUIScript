	MenuKeybindSetting.Size = UDim2.new(1, 0, 0.204999998, 0)

	MenuKeybindTitle.Name = "MenuKeybindTitle"
	MenuKeybindTitle.Parent = MenuKeybindSetting
	MenuKeybindTitle.BackgroundColor3 = Color3.fromRGB(157, 157, 157)
	MenuKeybindTitle.BackgroundTransparency = 1.000
	MenuKeybindTitle.BorderColor3 = Color3.fromRGB(0, 0, 0)
	MenuKeybindTitle.BorderSizePixel = 0
	MenuKeybindTitle.LayoutOrder = 1
	MenuKeybindTitle.Position = UDim2.new(0, 0, 0.235499918, 0)
	MenuKeybindTitle.Size = UDim2.new(0.533769131, 0, 0.529000044, 0)
	MenuKeybindTitle.ZIndex = 5
	MenuKeybindTitle.FontFace = Font.new([[rbxassetid://12187365977]], Enum.FontWeight.SemiBold, Enum.FontStyle.Normal);
	MenuKeybindTitle.Text = "Menu Keybind"
	MenuKeybindTitle.TextColor3 = Color3.fromRGB(255, 255, 255)
	MenuKeybindTitle.TextScaled = true
	MenuKeybindTitle.TextSize = 14.000
	MenuKeybindTitle.TextWrapped = true
	MenuKeybindTitle.TextXAlignment = Enum.TextXAlignment.Left

	MenuKeybindSettingUIListLayout.Name = "MenuKeybindSettingUIListLayout"
	MenuKeybindSettingUIListLayout.Parent = MenuKeybindSetting
