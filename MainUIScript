	BehindInfo2.BorderColor3 = Color3.fromRGB(0, 0, 0)
	BehindInfo2.BorderSizePixel = 0
	BehindInfo2.Position = UDim2.new(0.499999702, 0, 0.0625571609, 0)
	BehindInfo2.Size = UDim2.new(1, 0, 0.157115579, 0)
	BehindInfo2.ZIndex = 0
	BehindInfo2.BackgroundTransparency = 1

	BehindInfo1.Name = "Behind1"
	BehindInfo1.Parent = Info
	BehindInfo1.AnchorPoint = Vector2.new(0.5, 0.5)
	BehindInfo1.BackgroundColor3 = Color3.fromRGB(1, 17, 33)
	BehindInfo1.BorderColor3 = Color3.fromRGB(0, 0, 0)
	BehindInfo1.BorderSizePixel = 0
	BehindInfo1.Position = UDim2.new(0.599478126, 0, 0.509012222, 0)
	BehindInfo1.Size = UDim2.new(0.801043689, 0, 0.981972694, 0)
	BehindInfo1.ZIndex = 0
	BehindInfo1.BackgroundTransparency = 1

	MainUIAspectRatioConstraint.Parent = Main
	MainUIAspectRatioConstraint.AspectRatio = 1.505

	UserInputService.InputBegan:Connect(function(input)
		if input.KeyCode == toggleKey then
			local FrameToScale = Player:WaitForChild("PlayerGui"):WaitForChild("UILib"):WaitForChild("Main")
			local showMenu = tweenService:Create(FrameToScale.UIScale, tweenInfo2, { Scale = 1 })
			showMenu:Play()
			tweenService:Create(blur, tweenInfo2, {Size = 24}):Play()

			if FrameToScale.UIScale.Scale == 1 then
				local hideMenu = tweenService:Create(FrameToScale.UIScale, tweenInfo3, { Scale = 0 })
				tweenService:Create(blur, tweenInfo2, {Size = 0}):Play()
				hideMenu:Play()
			end
		end
	end)

	local frame = Player:WaitForChild("PlayerGui"):WaitForChild("UILib"):WaitForChild("Main")
	local dragToggle = nil
	local dragSpeed = 0.25
	local dragStart = nil
	local startPos = nil

	local function updateInput2(input)
		local delta = input.Position - dragStart
		local position = UDim2.new(startPos.X.Scale, startPos.X.Offset + delta.X,
			startPos.Y.Scale, startPos.Y.Offset + delta.Y)
		game:GetService('TweenService'):Create(frame, TweenInfo.new(dragSpeed), {Position = position}):Play()
	end

	frame.InputBegan:Connect(function(input)
		if (input.UserInputType == Enum.UserInputType.MouseButton1 or input.UserInputType == Enum.UserInputType.Touch) then 
			dragToggle = true
			dragStart = input.Position
			startPos = frame.Position
			input.Changed:Connect(function()
				if input.UserInputState == Enum.UserInputState.End then
					dragToggle = false
				end
			end)
		end
	end)

	UserInputService.InputChanged:Connect(function(input)
		if input.UserInputType == Enum.UserInputType.MouseMovement or input.UserInputType == Enum.UserInputType.Touch then
			if dragToggle then
				updateInput2(input)
			end
		end
	end)

	task.wait(0.4)
	tweenService:Create(Player:WaitForChild("PlayerGui"):WaitForChild("UILib"):WaitForChild("Main"):WaitForChild("UIScale"), tweenInfo2, {Scale = 1}):Play()
	tweenService:Create(blur, tweenInfo2, {Size = 24}):Play()
end

function shitware:AddSection(SectionName)
	local NewSection = Instance.new("Frame")
	local NewSectionUIListLayout = Instance.new("UIListLayout")
	local SectionTitle = Instance.new("TextLabel")
	local NewSectionUIPadding = Instance.new("UIPadding")
	local NewSectionUIAspectRatioConstraint = Instance.new("UIAspectRatioConstraint")
	local CurrentSections = Player:WaitForChild("PlayerGui"):WaitForChild("UILib"):WaitForChild("Main"):WaitForChild("Sections")

	NewSection.Name = SectionName
	NewSection.Parent = CurrentSections
	NewSection.AutomaticSize = 2
	NewSection.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
	NewSection.BackgroundTransparency = 1.000
	NewSection.BorderColor3 = Color3.fromRGB(0, 0, 0)
	NewSection.BorderSizePixel = 0
	NewSection.Position = UDim2.new(-1.49789003e-07, 0, 0, 0)
	NewSection.Size = UDim2.new(0.998094916, 0, 0.0547085591, 0)

	NewSectionUIListLayout.Parent = NewSection
