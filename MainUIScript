	MenuKeybindSettingUIPadding.PaddingLeft = UDim.new(0, 10)

	MenuKeybindSettingUIAspectRatioConstraint.Name = "MenuKeybindSettingUIAspectRatioConstraint"
	MenuKeybindSettingUIAspectRatioConstraint.Parent = MenuKeybindSetting
	MenuKeybindSettingUIAspectRatioConstraint.AspectRatio = 8.086

	MenuKeybindButton.Name = "MenuKeybindButton"
	MenuKeybindButton.Parent = MenuKeybindSetting
	MenuKeybindButton.BackgroundColor3 = Color3.fromRGB(7, 5, 9)
	MenuKeybindButton.BorderColor3 = Color3.fromRGB(0, 0, 0)
	MenuKeybindButton.BorderSizePixel = 0
	MenuKeybindButton.LayoutOrder = 2
	MenuKeybindButton.Size = UDim2.new(0.38499999, 0, 0.950999975, 0)
	MenuKeybindButton.Font = Enum.Font.SourceSans
	MenuKeybindButton.Text = ""
	MenuKeybindButton.TextColor3 = Color3.fromRGB(0, 0, 0)
	MenuKeybindButton.TextSize = 14.000

	MenuKeybindButtonUICorner.CornerRadius = UDim.new(0, 10)
	MenuKeybindButtonUICorner.Name = "MenuKeybindButtonUICorner"
	MenuKeybindButtonUICorner.Parent = MenuKeybindButton

	MenuKeybindButtonTitle.Name = "Title"
	MenuKeybindButtonTitle.Parent = MenuKeybindButton
	MenuKeybindButtonTitle.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
	MenuKeybindButtonTitle.BackgroundTransparency = 1.000
	MenuKeybindButtonTitle.BorderColor3 = Color3.fromRGB(0, 0, 0)
	MenuKeybindButtonTitle.BorderSizePixel = 0
	MenuKeybindButtonTitle.LayoutOrder = 2
	MenuKeybindButtonTitle.Position = UDim2.new(0, 0, 0.751723707, 0)
	MenuKeybindButtonTitle.Size = UDim2.new(1, 0, 0.600000024, 0)
	MenuKeybindButtonTitle.FontFace = Font.new([[rbxassetid://12187365977]], Enum.FontWeight.Bold, Enum.FontStyle.Normal);
	MenuKeybindButtonTitle.Text = "K"
	MenuKeybindButtonTitle.TextColor3 = Color3.fromRGB(255, 255, 255)
	MenuKeybindButtonTitle.TextScaled = true
	MenuKeybindButtonTitle.TextSize = 14.000
	MenuKeybindButtonTitle.TextWrapped = true

	MenuKeybindButtonUIListLayout.Name = "MenuKeybindButtonUIListLayout"
	MenuKeybindButtonUIListLayout.Parent = MenuKeybindButton
	MenuKeybindButtonUIListLayout.FillDirection = Enum.FillDirection.Horizontal
	MenuKeybindButtonUIListLayout.HorizontalAlignment = Enum.HorizontalAlignment.Center
	MenuKeybindButtonUIListLayout.SortOrder = Enum.SortOrder.LayoutOrder
	MenuKeybindButtonUIListLayout.VerticalAlignment = Enum.VerticalAlignment.Center

	MenuKeybindButtonUIAspectRatioConstraint.Name = "MenuKeybindButtonUIAspectRatioConstraint"
	MenuKeybindButtonUIAspectRatioConstraint.Parent = MenuKeybindButton
	MenuKeybindButtonUIAspectRatioConstraint.AspectRatio = 3.108

	SettingsCloseButton.MouseButton1Click:Connect(function()
		SettingsButtonOpen:SetAttribute("Visible", false)

		task.spawn(function()
			tweenService:Create(SettingsCloseButton.UIScale, tweenInfo2, { Scale = 1.1 }):Play()
			task.wait(0.05)
			tweenService:Create(SettingsCloseButton.UIScale, tweenInfo2, { Scale = 1 }):Play()
		end)

		tweenService:Create(SettingsUIScale, tweenInfo2, { Scale = 0 }):Play()
	end)

	SettingsButtonOpen.MouseButton1Click:Connect(function()
		SettingsButtonOpen:SetAttribute("Visible", not SettingsButtonOpen:GetAttribute("Visible"))

		task.spawn(function()
			tweenService:Create(SettingsButtonOpen.UIScale, tweenInfo2, { Scale = 1.1 }):Play()
			task.wait(0.05)
			tweenService:Create(SettingsButtonOpen.UIScale, tweenInfo2, { Scale = 1 }):Play()
		end)

		if SettingsButtonOpen:GetAttribute("Visible") == true then
			tweenService:Create(SettingsUIScale, tweenInfo2, { Scale = 1 }):Play()
		else
			tweenService:Create(SettingsUIScale, tweenInfo2, { Scale = 0 }):Play()
		end
	end)

	local function waitForKeyPress()
		local keyPressed = nil
		local connection

		local function onInputBegan(input, gameProcessed)
			if input.UserInputType == Enum.UserInputType.Keyboard then
				keyPressed = input.KeyCode
				connection:Disconnect()
			end
		end

		connection = UserInputService.InputBegan:Connect(onInputBegan)
		repeat
			wait()
		until keyPressed

		return keyPressed
	end

	-- Handle keybind change
	MenuKeybindButton.MouseButton1Click:Connect(function()
		MenuKeybindButtonTitle.Text = "Press any key."
		local newKeybind = waitForKeyPress()
		if newKeybind then
			MenuKeybindButtonTitle.Text = newKeybind.Name
			toggleKey = newKeybind
		end
	end)


	UserInputService.InputBegan:Connect(function(input)
		if input.KeyCode == toggleKey then
			local FrameToScale = Player:WaitForChild("PlayerGui"):WaitForChild("UILib"):WaitForChild("Main")
			local showMenu = tweenService:Create(FrameToScale.UIScale, tweenInfo2, { Scale = 1 })
			showMenu:Play()
			tweenService:Create(blur, tweenInfo2, {Size = 24}):Play()

			if FrameToScale.UIScale.Scale == 1 then
				local hideMenu = tweenService:Create(FrameToScale.UIScale, tweenInfo3, { Scale = 0 })
				tweenService:Create(blur, tweenInfo2, {Size = 0}):Play()
				hideMenu:Play()
			end
		end
	end)

	local frame = Player:WaitForChild("PlayerGui"):WaitForChild("UILib"):WaitForChild("Main")
	local dragToggle = nil
	local dragSpeed = 0.25
	local dragStart = nil
	local startPos = nil

	local function updateInput2(input)
		local delta = input.Position - dragStart
		local position = UDim2.new(startPos.X.Scale, startPos.X.Offset + delta.X,
			startPos.Y.Scale, startPos.Y.Offset + delta.Y)
		game:GetService('TweenService'):Create(frame, TweenInfo.new(dragSpeed), {Position = position}):Play()
	end

	frame.InputBegan:Connect(function(input)
		if (input.UserInputType == Enum.UserInputType.MouseButton1 or input.UserInputType == Enum.UserInputType.Touch) then 
			dragToggle = true
			dragStart = input.Position
			startPos = frame.Position
			input.Changed:Connect(function()
				if input.UserInputState == Enum.UserInputState.End then
					dragToggle = false
				end
			end)
		end
	end)

	UserInputService.InputChanged:Connect(function(input)
		if input.UserInputType == Enum.UserInputType.MouseMovement or input.UserInputType == Enum.UserInputType.Touch then
			if dragToggle then
				updateInput2(input)
			end
		end
	end)

	task.wait(0.4)
	tweenService:Create(Player:WaitForChild("PlayerGui"):WaitForChild("UILib"):WaitForChild("Main"):WaitForChild("UIScale"), tweenInfo2, {Scale = 1}):Play()
	tweenService:Create(blur, tweenInfo2, {Size = 24}):Play()
end

function shitware:AddTabSection(SectionName)
	local NewSection = Instance.new("Frame")
	local NewSectionUIListLayout = Instance.new("UIListLayout")
	local SectionTitle = Instance.new("TextLabel")
	local NewSectionUIPadding = Instance.new("UIPadding")
	local NewSectionUIAspectRatioConstraint = Instance.new("UIAspectRatioConstraint")
	local CurrentSections = Player:WaitForChild("PlayerGui"):WaitForChild("UILib"):WaitForChild("Main"):WaitForChild("Sections")

	NewSection.Name = SectionName
	NewSection.Parent = CurrentSections
	NewSection.AutomaticSize = 2
	NewSection.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
	NewSection.BackgroundTransparency = 1.000
	NewSection.BorderColor3 = Color3.fromRGB(0, 0, 0)
	NewSection.BorderSizePixel = 0
	NewSection.Position = UDim2.new(-1.49789003e-07, 0, 0, 0)
	NewSection.Size = UDim2.new(0.998094916, 0, 0.0547085591, 0)

	NewSectionUIListLayout.Parent = NewSection
	NewSectionUIListLayout.SortOrder = Enum.SortOrder.LayoutOrder
	NewSectionUIListLayout.Padding = UDim.new(0, 3)

	SectionTitle.Name = "SectionTitle"
	SectionTitle.Parent = NewSection
	SectionTitle.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
	SectionTitle.BackgroundTransparency = 1.000
	SectionTitle.BorderColor3 = Color3.fromRGB(0, 0, 0)
	SectionTitle.BorderSizePixel = 0
	SectionTitle.LayoutOrder = 1
	SectionTitle.Position = UDim2.new(0, 0, -3.9593732e-07, 0)
	SectionTitle.Size = UDim2.new(1.00190759, 0, 0.604787946, 0)
	SectionTitle.FontFace = Font.new([[rbxassetid://12187365977]], Enum.FontWeight.Bold, Enum.FontStyle.Normal);
	SectionTitle.Text = SectionName
	SectionTitle.TextColor3 = Color3.fromRGB(157, 157, 157)
	SectionTitle.TextScaled = true
	SectionTitle.TextSize = 14.000
	SectionTitle.TextWrapped = true
	SectionTitle.TextXAlignment = Enum.TextXAlignment.Left

	NewSectionUIPadding.Parent = SectionTitle
	NewSectionUIPadding.PaddingLeft = UDim.new(0, 15)

	NewSectionUIAspectRatioConstraint.Parent = SectionTitle
	NewSectionUIAspectRatioConstraint.AspectRatio = 11.881
end

function shitware:AddTab(SectionName, ButtonText, ImageName, Call)
	local Section = Player:WaitForChild("PlayerGui"):WaitForChild("UILib"):WaitForChild("Main"):WaitForChild("Sections"):FindFirstChild(SectionName)

	if Section then
		local NewSectionButton = Instance.new("TextButton")
		local NewSectionButtonUIListLayout = Instance.new("UIListLayout")
		local NewSectionButtonUICorner = Instance.new("UICorner")
		local NewSectionButtonImageLabel = Instance.new("ImageLabel")
		local NewSectionButtonImageLabelUIAspectRatioConstraint = Instance.new("UIAspectRatioConstraint")
		local NewSectionButtonUIAspectRatioConstraint = Instance.new("UIAspectRatioConstraint")
		local ButtonNameText = Instance.new("TextLabel")
		local UIScale = Instance.new("UIScale")
		local NewTab = Instance.new("ScrollingFrame")
		local NewTabUIPadding = Instance.new("UIPadding")
		local NewTabUIGridLayout = Instance.new("UIGridLayout")
		local ShowingFrame = Instance.new("Frame")
		local ShowingFrameUICorner = Instance.new("UICorner")
		local ShowingFrameUIScale = Instance.new("UIScale")
		local ShowingFrameUIAspectRatioConstraint = Instance.new("UIAspectRatioConstraint")

		NewTab.Name = ButtonText
		NewTab.Parent = Player:WaitForChild("PlayerGui"):WaitForChild("UILib"):WaitForChild("Main"):WaitForChild("TabHolder")
		NewTab.Active = true
		NewTab.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
		NewTab.ScrollBarImageColor3 = Color3.fromRGB(63, 136, 252)
		NewTab.BackgroundTransparency = 1.000
		NewTab.BorderColor3 = Color3.fromRGB(0, 0, 0)
		NewTab.BorderSizePixel = 0
		NewTab.Position = UDim2.new(0.0130000003, 0, 0.119999997, 0)
		NewTab.Size = UDim2.new(0.97299999, 0, 0.870000005, 0)
		NewTab.ScrollBarThickness = 8
		NewTab.Visible = false
		NewTab.ZIndex = 200
		NewTab:SetAttribute("Visible", false)

		NewTabUIPadding.Parent = NewTab
		NewTabUIPadding.PaddingBottom = UDim.new(0, 5)
		NewTabUIPadding.PaddingLeft = UDim.new(0, 5)
		NewTabUIPadding.PaddingRight = UDim.new(0, 5)
		NewTabUIPadding.PaddingTop = UDim.new(0, 10)

		NewTabUIGridLayout.Parent = NewTab
		NewTabUIGridLayout.SortOrder = Enum.SortOrder.LayoutOrder
		NewTabUIGridLayout.CellPadding = UDim2.new(0.0399999991, 0, 0.0399999991, 0)
		NewTabUIGridLayout.CellSize = UDim2.new(0.479999989, 0, 0.25, 0)

		NewSectionButton.Name = "NewSectionButton"
		NewSectionButton.Parent = Section
		NewSectionButton.BackgroundColor3 = Color3.fromRGB(31, 31, 31)
		NewSectionButton.BackgroundTransparency = 1.000
		NewSectionButton.BorderColor3 = Color3.fromRGB(0, 0, 0)
		NewSectionButton.BorderSizePixel = 0
		NewSectionButton.LayoutOrder = 2
		NewSectionButton.Position = UDim2.new(1.55051694e-07, 0, 0.223096818, 0)
		NewSectionButton.Size = UDim2.new(1.00972903, 0, 0.630487919, 0)
		NewSectionButton.Font = Enum.Font.SourceSans
		NewSectionButton.Text = ""
		NewSectionButton.TextColor3 = Color3.fromRGB(0, 0, 0)
		NewSectionButton.TextScaled = true
		NewSectionButton.TextSize = 14.000
		NewSectionButton.TextWrapped = true
		UIScale.Parent = NewSectionButton

		ShowingFrame.Name = "ShowingFrame"
		ShowingFrame.Parent = NewSectionButton
		ShowingFrame.BackgroundColor3 = Color3.fromRGB(17, 141, 203)
		ShowingFrame.BorderColor3 = Color3.fromRGB(0, 0, 0)
		ShowingFrame.BorderSizePixel = 0
		ShowingFrame.LayoutOrder = 1
		ShowingFrame.Position = UDim2.new(0.027301779, 0, 0.0890263766, 0)
		ShowingFrame.Size = UDim2.new(0.026, 0, 0.652, 0)
		ShowingFrame.Visible = true

		ShowingFrameUICorner.CornerRadius = UDim.new(0, 20)
		ShowingFrameUICorner.Parent = ShowingFrame

		ShowingFrameUIScale.Scale = 0
		ShowingFrameUIScale.Parent = ShowingFrame

		ShowingFrameUIAspectRatioConstraint.Parent = ShowingFrame
		ShowingFrameUIAspectRatioConstraint.AspectRatio = 0.237

		NewSectionButtonUIListLayout.Parent = NewSectionButton
		NewSectionButtonUIListLayout.FillDirection = Enum.FillDirection.Horizontal
		NewSectionButtonUIListLayout.HorizontalAlignment = Enum.HorizontalAlignment.Center
		NewSectionButtonUIListLayout.SortOrder = Enum.SortOrder.LayoutOrder
		NewSectionButtonUIListLayout.VerticalAlignment = Enum.VerticalAlignment.Center
		NewSectionButtonUIListLayout.Padding = UDim.new(0, 7)

		ButtonNameText.Name = "ButtonNameText"
		ButtonNameText.Parent = NewSectionButton
		ButtonNameText.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
		ButtonNameText.BackgroundTransparency = 1.000
		ButtonNameText.BorderColor3 = Color3.fromRGB(0, 0, 0)
		ButtonNameText.BorderSizePixel = 0
		ButtonNameText.LayoutOrder = 3
		ButtonNameText.Position = UDim2.new(0.375488073, 0, 0.234592378, 0)
		ButtonNameText.Size = UDim2.new(0.694048345, 0, 0.530814826, 0)
		ButtonNameText.FontFace = Font.new([[rbxassetid://12187365977]], Enum.FontWeight.SemiBold, Enum.FontStyle.Normal);
		ButtonNameText.Text = ButtonText
		ButtonNameText.TextColor3 = Color3.fromRGB(157, 157, 157)
		ButtonNameText.TextScaled = true
		ButtonNameText.TextSize = 14.000
		ButtonNameText.TextWrapped = true
		ButtonNameText.TextXAlignment = Enum.TextXAlignment.Left

		NewSectionButtonUICorner.CornerRadius = UDim.new(0, 10)
		NewSectionButtonUICorner.Parent = NewSectionButton

		NewSectionButtonImageLabel.Parent = NewSectionButton
		NewSectionButtonImageLabel.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
		NewSectionButtonImageLabel.BackgroundTransparency = 1.000
		NewSectionButtonImageLabel.BorderColor3 = Color3.fromRGB(0, 0, 0)
		NewSectionButtonImageLabel.BorderSizePixel = 0
		NewSectionButtonImageLabel.Position = UDim2.new(0.0504929796, 0, 0.12427076, 0)
		NewSectionButtonImageLabel.Size = UDim2.new(0.167615026, 0, 0.732507408, 0)
		NewSectionButtonImageLabel.Image = Icons[ImageName] or Icons["home"]
		NewSectionButtonImageLabel.ImageColor3 = Color3.fromRGB(9, 152, 227)
		NewSectionButtonImageLabel.LayoutOrder = 2

		NewSectionButtonImageLabelUIAspectRatioConstraint.Parent = NewSectionButtonImageLabel
		NewSectionButtonImageLabelUIAspectRatioConstraint.AspectRatio = 1.013

		NewSectionButtonUIAspectRatioConstraint.Parent = NewSectionButton
		NewSectionButtonUIAspectRatioConstraint.AspectRatio = 5.938

		NewSectionButton.MouseButton1Click:Connect(function()
			local TabHolder = Player:WaitForChild("PlayerGui"):WaitForChild("UILib"):WaitForChild("Main"):WaitForChild("TabHolder")
			for _, ScrollingFrame in TabHolder:GetChildren() do
				if ScrollingFrame:IsA("ScrollingFrame") then
					if ScrollingFrame.Name == ButtonText then
						--NOTHING
					else
						print("Hiding")
						tweenService:Create(ScrollingFrame, ScrollingFrametweenInfo, {Position = UDim2.new(0.0130000003, 0, 1.119999997, 0)}):Play()
						ScrollingFrame.Position = UDim2.new(0.0130000003, 0, 0.119999997, 0)
						ScrollingFrame.Visible = false
						ScrollingFrame:SetAttribute("Visible", false)
					end
				end
			end

			TabHolder:FindFirstChild(ButtonText):SetAttribute("Visible", not TabHolder:FindFirstChild(ButtonText):GetAttribute("Visible"))

			if TabHolder:FindFirstChild(ButtonText):GetAttribute("Visible") == true then
				TabHolder:FindFirstChild(ButtonText).Position = UDim2.new(0.0130000003, 0, 1.119999997, 0)
				TabHolder:FindFirstChild(ButtonText).Visible = true
				tweenService:Create(TabHolder:FindFirstChild(ButtonText), ScrollingFrametweenInfo, {Position = UDim2.new(0.0130000003, 0, 0.119999997, 0)}):Play()
			else
				local Hide = tweenService:Create(TabHolder:FindFirstChild(ButtonText), ScrollingFrametweenInfo, {Position = UDim2.new(0.0130000003, 0, 1.119999997, 0)})
				Hide.Completed:Connect(function()
					TabHolder:FindFirstChild(ButtonText).Visible = false
				end)
				Hide:Play()
				TabHolder:FindFirstChild(ButtonText).Position = UDim2.new(0.0130000003, 0, 0.119999997, 0)
			end

		end)

		task.spawn(function()
			while wait() do
				local TabHolder = Player:WaitForChild("PlayerGui"):WaitForChild("UILib"):WaitForChild("Main"):WaitForChild("TabHolder")

				if TabHolder:FindFirstChild(ButtonText):GetAttribute("Visible") == true then
					tweenService:Create(NewSectionButton.ShowingFrame.UIScale, tweenInfo, {Scale = 1}):Play()
					local UiScale = NewSectionButton.UIScale
					local scale = 1.1
					tweenService:Create(ButtonNameText, tweenInfo, {TextColor3 = Color3.fromRGB(255, 255, 255)}):Play()
					tweenService:Create(UiScale, tweenInfo, {Scale = scale}):Play()
				elseif TabHolder:FindFirstChild(ButtonText):GetAttribute("Visible") == false then
					tweenService:Create(NewSectionButton.ShowingFrame.UIScale, tweenInfo, {Scale = 0}):Play()
					local UiScale = NewSectionButton.UIScale
					local scale = 1
					tweenService:Create(ButtonNameText, tweenInfo, {TextColor3 = Color3.fromRGB(157, 157, 157)}):Play()
					tweenService:Create(UiScale, tweenInfo, {Scale = scale}):Play()
				end
			end
		end)
	end
end

function shitware:AddTabBox(TabName, TabBoxName)
	local Tab = Player:WaitForChild("PlayerGui"):WaitForChild("UILib"):WaitForChild("Main"):WaitForChild("TabHolder"):FindFirstChild(TabName)

	if Tab then
		local TabBox = Instance.new("Frame")
		local TabBoxUICorner = Instance.new("UICorner")
		local TabBoxTopBar = Instance.new("Frame")
		local TabNameTextUIAspectRatioConstraint = Instance.new("UIAspectRatioConstraint")
		local TabBoxTopBarTopBarStripe = Instance.new("Frame")
		local TabBoxTopBarTopBarStripeUIAspectRatioConstraint = Instance.new("UIAspectRatioConstraint")
		local TabBoxTopBarUIAspectRatioConstraint = Instance.new("UIAspectRatioConstraint")
		local TabBoxFunctions = Instance.new("ScrollingFrame")
		local TabBoxFunctionsUIListLayout = Instance.new("UIListLayout")
		local TabNameText = Instance.new("TextLabel")
		local TabBoxUIStroke = Instance.new("UIStroke")

		TabBox.Name = TabBoxName
		TabBox.Parent = Tab
		TabBox.AnchorPoint = Vector2.new(0.5, 0.5)
		TabBox.BackgroundColor3 = Color3.fromRGB(7, 5, 9)
		TabBox.BorderColor3 = Color3.fromRGB(0, 0, 0)
		TabBox.BorderSizePixel = 0
		TabBox.Position = UDim2.new(0.5, 0, 0.5, 0)
		TabBox.Size = UDim2.new(1, 0, 1, 0)

		TabBoxUIStroke.ApplyStrokeMode = 1
