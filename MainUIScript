		TabBoxTopBarTopBarStripe.Size = UDim2.new(0.949999988, 0, 0.0240000021, 0)

		TabBoxTopBarTopBarStripeUIAspectRatioConstraint.Parent = TabBoxTopBarTopBarStripe
		TabBoxTopBarTopBarStripeUIAspectRatioConstraint.AspectRatio = 218.941

		TabBoxTopBarUIAspectRatioConstraint.Parent = TabBoxTopBar
		TabBoxTopBarUIAspectRatioConstraint.AspectRatio = 5.531

		TabBoxFunctions.Name = "Functions"
		TabBoxFunctions.Parent = TabBox
		TabBoxFunctions.Active = true
		TabBoxFunctions.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
		TabBoxFunctions.BackgroundTransparency = 1.000
		TabBoxFunctions.BorderColor3 = Color3.fromRGB(0, 0, 0)
		TabBoxFunctions.BorderSizePixel = 0
		TabBoxFunctions.Position = UDim2.new(0.0269998871, 0, 0.182000056, 0)
		TabBoxFunctions.Size = UDim2.new(0.944000006, 0, 0.778016746, 0)
		TabBoxFunctions.BottomImage = ""
		TabBoxFunctions.ScrollBarThickness = 0

		TabBoxFunctionsUIListLayout.Parent = TabBoxFunctions
		TabBoxFunctionsUIListLayout.HorizontalAlignment = Enum.HorizontalAlignment.Center
		TabBoxFunctionsUIListLayout.SortOrder = Enum.SortOrder.LayoutOrder
		TabBoxFunctionsUIListLayout.Padding = UDim.new(0, 5)
	end
end

function shitware:AddToggle(TabBoxName, ToggleText, callback)
	local Tab
	for i,v in Player:WaitForChild("PlayerGui"):WaitForChild("UILib"):WaitForChild("Main"):WaitForChild("TabHolder"):GetChildren() do
		if v:FindFirstChild(TabBoxName) then
			Tab = v:FindFirstChild(TabBoxName)
		end
	end

	if Tab then
		local NewToggle = Instance.new("Frame")
		local NewToggleName = Instance.new("TextLabel")
		local NewToggleUIListLayout = Instance.new("UIListLayout")
		local NewToggleUIPadding = Instance.new("UIPadding")
		local NewToggleBG = Instance.new("Frame")
		local NewToggleUICorner = Instance.new("UICorner")
		local KNOB = Instance.new("TextButton")
		local KnobUICorner = Instance.new("UICorner")
		local KNOBUIAspectRatioConstraint = Instance.new("UIAspectRatioConstraint")
		local NewToggleBGUIAspectRatioConstraint = Instance.new("UIAspectRatioConstraint")
		local NewToggleUIAspectRatioConstraint = Instance.new("UIAspectRatioConstraint")

		NewToggle.Name = "Toggle"
		NewToggle.Parent = Tab.Functions
		NewToggle.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
		NewToggle.BackgroundTransparency = 1.000
		NewToggle.BorderColor3 = Color3.fromRGB(0, 0, 0)
		NewToggle.BorderSizePixel = 0
		NewToggle.Position = UDim2.new(0, 0, 0, 0)
		NewToggle.Size = UDim2.new(1, 0, 0.205, 0)
		NewToggle:SetAttribute("Toggle", false)

		NewToggleName.Name = TabBoxName
		NewToggleName.Parent = NewToggle
		NewToggleName.BackgroundColor3 = Color3.fromRGB(157, 157, 157)
		NewToggleName.BackgroundTransparency = 1.000
		NewToggleName.BorderColor3 = Color3.fromRGB(0, 0, 0)
		NewToggleName.BorderSizePixel = 0
		NewToggleName.LayoutOrder = 1
		NewToggleName.Position = UDim2.new(0, 0, 0.23570247, 0)
		NewToggleName.Size = UDim2.new(0.803, 0, 0.529, 0)
		NewToggleName.ZIndex = 5
		NewToggleName.FontFace = Font.new([[rbxassetid://12187365977]], Enum.FontWeight.SemiBold, Enum.FontStyle.Normal);
		NewToggleName.Text = ToggleText
		NewToggleName.TextColor3 = Color3.fromRGB(255, 255, 255)
		NewToggleName.TextScaled = true
		NewToggleName.TextSize = 14.000
		NewToggleName.TextWrapped = true
		NewToggleName.TextXAlignment = Enum.TextXAlignment.Left

		NewToggleUIListLayout.Parent = NewToggle
		NewToggleUIListLayout.FillDirection = Enum.FillDirection.Horizontal
		NewToggleUIListLayout.SortOrder = Enum.SortOrder.LayoutOrder
		NewToggleUIListLayout.VerticalAlignment = Enum.VerticalAlignment.Center

		NewToggleUIPadding.Parent = NewToggle
		NewToggleUIPadding.PaddingLeft = UDim.new(0, 10)

		NewToggleBG.Name = "ToggleBG"
		NewToggleBG.Parent = NewToggle
		NewToggleBG.BackgroundColor3 = Color3.fromRGB(47, 47, 47)
		NewToggleBG.BorderColor3 = Color3.fromRGB(0, 0, 0)
		NewToggleBG.BorderSizePixel = 0
		NewToggleBG.LayoutOrder = 2
		NewToggleBG.Position = UDim2.new(0.692625642, 0, 0.169966787, 0)
		NewToggleBG.Size = UDim2.new(0.163265303, 0, 0.660066009, 0)

		NewToggleUICorner.CornerRadius = UDim.new(1, 0)
		NewToggleUICorner.Parent = NewToggleBG

		KNOB.Name = "KNOB"
		KNOB.Parent = NewToggleBG
		KNOB.AnchorPoint = Vector2.new(0.5, 0.5)
		KNOB.BackgroundColor3 = Color3.fromRGB(0, 169, 247)
		KNOB.BorderColor3 = Color3.fromRGB(0, 0, 0)
		KNOB.BorderSizePixel = 0
		KNOB.Position = UDim2.new(0.200000003, 0, 0.5, 0)
		KNOB.Size = UDim2.new(0.469362944, 0, 1.30041063, 0)
		KNOB.Font = Enum.Font.SourceSans
		KNOB.Text = ""
		KNOB.TextColor3 = Color3.fromRGB(0, 0, 0)
		KNOB.TextSize = 14.000

		KnobUICorner.CornerRadius = UDim.new(1, 0)
		KnobUICorner.Parent = KNOB

		KNOBUIAspectRatioConstraint.Parent = KNOB

		NewToggleBGUIAspectRatioConstraint.Parent = NewToggleBG
		NewToggleBGUIAspectRatioConstraint.AspectRatio = 1.918

		NewToggleUIAspectRatioConstraint.Parent = NewToggle
		NewToggleUIAspectRatioConstraint.AspectRatio = 8.086

		KNOB.MouseButton1Click:Connect(function()
			KNOB:SetAttribute("Toggle", not KNOB:GetAttribute("Toggle"))

			if KNOB:GetAttribute("Toggle") == true then
				tweenService:Create(KNOB, tweenInfo, {Position = UDim2.new(0.8, 0, 0.5, 0)}):Play()
				tweenService:Create(KNOB.Parent, tweenInfo, {BackgroundColor3 = Color3.fromRGB(0, 22, 42)}):Play()
			else
				tweenService:Create(KNOB, tweenInfo, {Position = UDim2.new(0.2, 0, 0.5, 0)}):Play()
				tweenService:Create(KNOB.Parent, tweenInfo, {BackgroundColor3 = Color3.fromRGB(47, 47, 47)}):Play()
			end

			if callback then
				callback(KNOB:GetAttribute("Toggle"))
			end
		end)
	end
end

function shitware:AddSlider(TabBoxName, SliderText, MinValue, MaxValue, callback)
	local Tab
	for i,v in Player:WaitForChild("PlayerGui"):WaitForChild("UILib"):WaitForChild("Main"):WaitForChild("TabHolder"):GetChildren() do
		if v:FindFirstChild(TabBoxName) then
			Tab = v:FindFirstChild(TabBoxName)
		end
	end

	if Tab then
		local SliderFunction = Instance.new("Frame")
		local SliderTextLabel = Instance.new("TextLabel")
		local SliderUIListLayout = Instance.new("UIListLayout")
		local SliderUIPadding = Instance.new("UIPadding")
		local SliderBG = Instance.new("Frame")
		local SliderBGUICorner = Instance.new("UICorner")
		local Slider = Instance.new("TextButton")
		local UICorner_2 = Instance.new("UICorner")
		local SliderBGUIAspectRatioConstraint = Instance.new("UIAspectRatioConstraint")
		local SliderFunctionUIAspectRatioConstraint = Instance.new("UIAspectRatioConstraint")
		local Trigger = Instance.new("TextButton")
		local TriggerUIAspectRatioConstraint = Instance.new("UIAspectRatioConstraint")
		local SliderAmount = Instance.new("TextLabel")
		local UIAspectRatioConstraint = Instance.new("UIAspectRatioConstraint")

		SliderFunction.Name = "Slider"
		SliderFunction.Parent = Tab.Functions
		SliderFunction.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
		SliderFunction.BackgroundTransparency = 1.000
		SliderFunction.BorderColor3 = Color3.fromRGB(0, 0, 0)
		SliderFunction.BorderSizePixel = 0
		SliderFunction.Position = UDim2.new(0, 0, 0.0621259548, 0)
		SliderFunction.Size = UDim2.new(1, 0, 0.276441187, 0)

		SliderTextLabel.Name = "SliderText"
		SliderTextLabel.Parent = SliderFunction
		SliderTextLabel.BackgroundColor3 = Color3.fromRGB(157, 157, 157)
		SliderTextLabel.BackgroundTransparency = 1.000
		SliderTextLabel.BorderColor3 = Color3.fromRGB(0, 0, 0)
		SliderTextLabel.BorderSizePixel = 0
		SliderTextLabel.LayoutOrder = 1
		SliderTextLabel.Position = UDim2.new(0, 0, 0.235500291, 0)
		SliderTextLabel.Size = UDim2.new(0.598377883, 0, 0.528999984, 0)
		SliderTextLabel.ZIndex = 5
		SliderTextLabel.FontFace = Font.new([[rbxassetid://12187365977]], Enum.FontWeight.SemiBold, Enum.FontStyle.Normal);
		SliderTextLabel.Text = SliderText
		SliderTextLabel.TextColor3 = Color3.fromRGB(255, 255, 255)
		SliderTextLabel.TextScaled = true
		SliderTextLabel.TextSize = 14.000
		SliderTextLabel.TextWrapped = true
		SliderTextLabel.TextXAlignment = Enum.TextXAlignment.Left

		SliderUIListLayout.Parent = SliderFunction
		SliderUIListLayout.FillDirection = Enum.FillDirection.Horizontal
		SliderUIListLayout.SortOrder = Enum.SortOrder.LayoutOrder
		SliderUIListLayout.VerticalAlignment = Enum.VerticalAlignment.Center

		SliderUIPadding.Parent = SliderFunction
		SliderUIPadding.PaddingLeft = UDim.new(0, 10)

		SliderBG.Name = "SliderBG"
		SliderBG.Parent = SliderFunction
		SliderBG.BackgroundColor3 = Color3.fromRGB(11, 81, 244)
		SliderBG.BorderColor3 = Color3.fromRGB(0, 0, 0)
		SliderBG.BorderSizePixel = 0
		SliderBG.LayoutOrder = 2
		SliderBG.Position = UDim2.new(0.563442409, 0, 0.367229193, 0)
		SliderBG.Size = UDim2.new(0.345646083, 0, 0.265542328, 0)

		SliderBGUICorner.CornerRadius = UDim.new(0, 25)
		SliderBGUICorner.Parent = SliderBG

		Slider.Name = "Slider"
		Slider.Parent = SliderBG
		Slider.BackgroundColor3 = Color3.fromRGB(0, 169, 247)
		Slider.BorderColor3 = Color3.fromRGB(0, 0, 0)
		Slider.BorderSizePixel = 0
		Slider.Size = UDim2.new(0.400000006, 0, 1, 0)
		Slider.Font = Enum.Font.SourceSans
		Slider.Text = ""
		Slider.TextColor3 = Color3.fromRGB(0, 0, 0)
		Slider.TextSize = 14.000

		UICorner_2.CornerRadius = UDim.new(0, 25)
		UICorner_2.Parent = Slider

		SliderBGUIAspectRatioConstraint.Parent = SliderBG
		SliderBGUIAspectRatioConstraint.AspectRatio = 10.025

		SliderAmount.Name = "SliderAmount"
		SliderAmount.Parent = SliderBG
		SliderAmount.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
		SliderAmount.BackgroundTransparency = 1.000
		SliderAmount.BorderColor3 = Color3.fromRGB(0, 0, 0)
		SliderAmount.BorderSizePixel = 0
		SliderAmount.Position = UDim2.new(0, 0, -2.04099989, 0)
		SliderAmount.Size = UDim2.new(1, 0, 1.46399999, 0)
		SliderAmount.FontFace = Font.new([[rbxassetid://12187365977]], Enum.FontWeight.Bold, Enum.FontStyle.Normal);
		SliderAmount.Text = "1"
		SliderAmount.TextColor3 = Color3.fromRGB(181, 181, 181)
		SliderAmount.TextScaled = true
		SliderAmount.TextSize = 14.000
		SliderAmount.TextWrapped = true

		UIAspectRatioConstraint.Parent = SliderAmount
		UIAspectRatioConstraint.AspectRatio = 6.848

		Trigger.Name = "Trigger"
		Trigger.Parent = SliderBG
		Trigger.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
		Trigger.BackgroundTransparency = 1.000
		Trigger.BorderColor3 = Color3.fromRGB(0, 0, 0)
		Trigger.BorderSizePixel = 0
		Trigger.Size = UDim2.new(1, 0, 1, 0)
		Trigger.Font = Enum.Font.SourceSans
		Trigger.Text = ""
		Trigger.TextColor3 = Color3.fromRGB(0, 0, 0)
		Trigger.TextSize = 14.000

		TriggerUIAspectRatioConstraint.Parent = Trigger
		TriggerUIAspectRatioConstraint.AspectRatio = 10.025

		SliderFunctionUIAspectRatioConstraint.Parent = SliderFunction
		SliderFunctionUIAspectRatioConstraint.AspectRatio = 8.086

		local TweenStyle = TweenInfo.new(0.25, Enum.EasingStyle.Exponential)

		local function UpdateSlider()
			local MinValueClamped = MinValue/100
			local MaxValueClamped = MaxValue/100

			local output = math.clamp((mouse.X-SliderBG.AbsolutePosition.X)/SliderBG.AbsoluteSize.X,0,1)

			local outputClamped = MinValue + (output*(MaxValue-MinValue))

			if tonumber(SliderAmount.Text) ~= outputClamped then
				tweenService:Create(Slider, TweenStyle, {Size = UDim2.fromScale(output, 1)}):Play()
			end

			SliderAmount.Text = math.round(tonumber(outputClamped))

			if callback then
				callback(math.round(tonumber(outputClamped)))
			end
		end

		local sliderActive = false

		local function ActivateSlider()
			sliderActive = true
			while sliderActive do
				UpdateSlider()
				task.wait()
			end
		end

		Trigger.MouseButton1Down:Connect(ActivateSlider)

		UserInputService.InputEnded:Connect(function(input)
			if input.UserInputType == Enum.UserInputType.MouseButton1 or input.UserInputType == Enum.UserInputType.Touch then
				sliderActive = false
			end
		end)
	end
end

return shitware
